generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model loader {
  id String @unique
}

model prompt {
  id     Int      @id @unique @default(autoincrement())
  prompt String   @db.Text
  date   DateTime
}

model response {
  id                     Int     @id @unique @default(autoincrement())
  run_id                 String
  platform               String
  model                  String
  processing_time        Int
  diagnostic_validation  Boolean
  answer_followed_prompt Boolean
  temperature            String
  disagreement           String
  disagreement_count     Int
  error                  String
  likert                 Int
  input_tokens           Int
  output_tokens          Int
  icd9_codes_count       Int
  disagreement_rate      Float
}

model admissions {
  id                   Int    @id @unique @default(autoincrement())
  row_id               String
  subject_id           String
  hadm_id              String
  admittime            String
  dischtime            String
  deathtime            String
  admission_type       String
  admission_location   String
  discharge_location   String
  insurance            String
  language             String
  religion             String
  marital_status       String
  ethnicity            String
  edregtime            String
  edouttime            String
  diagnosis            String
  hospital_expire_flag String
  has_chartevents_data String
}

model patients {
  id          Int    @id @unique @default(autoincrement())
  row_id      String
  subject_id  String
  gender      String
  dob         String
  dod         String
  dod_hosp    String
  dod_ssn     String
  expire_flag String
}

model diagnoses_icd {
  id         Int    @id @unique @default(autoincrement())
  row_id     String
  subject_id String
  hadm_id    String
  seq_num    String
  icd9_code  String
}

model d_icd_diagnoses {
  id          Int    @id @unique @default(autoincrement())
  row_id      String
  icd9_code   String
  short_title String
  long_title  String
}

